{
  "manifest_version": 2,
  "name": "Hide GitHub Reactions",
  "version": "1.0",

  "description": "Keeps all Github reactions out of your gaze",

  "icons": {
    "64": "icons/closed-eye-64.png"
  },

  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["hide-reactions.css"]
    }
  ]
}
